    "Tool": {
      "type": "string",
      "const": "qeeg_nf_cli"
    },
    "Version": {
      "type": "string"
    },
    "GitDescribe": {
      "type": "string"
    },
    "BuildType": {
      "type": "string"
    },
    "Compiler": {
      "type": "string"
    },
    "CppStandard": {
      "type": "string"
    },
    "TimestampLocal": {
      "type": "string"
    },
    "OutputDir": {
      "type": "string"
    },
    "Outputs": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "demo": {
      "type": "boolean"
    },
    "input_path": {
      "type": "string"
    },
    "channel_qc": {
      "type": [
        "string",
        "null"
      ]
    },
    "channel_qc_resolved": {
      "type": [
        "string",
        "null"
      ]
    },
    "qc_bad_channel_count": {
      "type": "integer",
