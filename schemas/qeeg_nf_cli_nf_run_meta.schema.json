    "log10_power",
    "relative_power",
    "relative_fmin_hz",
    "relative_fmax_hz",
    "overlap",
    "artifact_gate",
    "artifact_ptp_z",
    "artifact_rms_z",
    "artifact_kurtosis_z",
    "artifact_min_bad_channels",
    "artifact_ignore_channels",
    "biotrace_ui",
    "export_derived_events",
    "derived_events_written",
    "derived_events_csv",
    "derived_events_tsv",
    "derived_events_json"
  ],
  "properties": {
    "$schema": {
      "type": "string",
      "description": "Optional schema hint for editors/validators (points at this schema's $id).",
      "const": "https://raw.githubusercontent.com/masterblaster1999/qeeg-neurofeedback-opensoftware/main/schemas/qeeg_nf_cli_nf_run_meta.schema.json"
