    "Version",
    "GitDescribe",
    "BuildType",
    "Compiler",
    "CppStandard",
    "TimestampLocal",
    "OutputDir",
    "Outputs",
    "demo",
    "input_path",
    "channel_qc",
    "channel_qc_resolved",
    "qc_bad_channel_count",
    "qc_bad_channels",
    "allow_bad_metric_channels",
    "fs_hz",
    "protocol",
    "metric_spec",
    "band_spec",
    "reward_direction",
    "threshold_init",
    "baseline_seconds",
    "baseline_quantile",
    "baseline_quantile_used",
    "target_reward_rate",
    "adapt_eta",
    "adapt_mode",
    "adapt_interval_seconds",
    "adapt_window_seconds",
    "adapt_min_samples",
    "reward_rate_window_seconds",
    "reward_on_frames",
    "reward_off_frames",
    "threshold_hysteresis",
    "dwell_seconds",
    "refractory_seconds",
