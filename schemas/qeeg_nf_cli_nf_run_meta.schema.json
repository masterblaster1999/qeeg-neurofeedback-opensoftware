    },
    "qc_bad_channels": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "allow_bad_metric_channels": {
      "type": "boolean"
    },
    "fs_hz": {
      "type": "number",
      "minimum": 0
    },
    "protocol": {
      "type": [
        "string",
        "null"
      ]
    },
    "metric_spec": {
      "type": "string"
    },
    "band_spec": {
      "type": "string"
    },
    "reward_direction": {
      "type": "string",
      "enum": [
        "above",
        "below"
      ]
    },
    "threshold_init": {
      "type": [
        "number",
        "null"
      ]
    },
    "baseline_seconds": {
      "type": "number",
      "minimum": 0
    },
    "baseline_quantile": {
      "type": [
        "number",
        "null"
      ]
    },
    "baseline_quantile_used": {
      "type": "number"
    },
    "target_reward_rate": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    },
    "adapt_eta": {
      "type": "number",
      "minimum": 0
    },
    "adapt_mode": {
      "type": "string"
    },
    "adapt_interval_seconds": {
      "type": "number",
      "minimum": 0
    },
    "adapt_window_seconds": {
      "type": "number",
      "minimum": 0
    },
    "adapt_min_samples": {
      "type": "integer",
      "minimum": 0
    },
    "reward_rate_window_seconds": {
      "type": "number",
      "minimum": 0
    },
    "reward_on_frames": {
      "type": "integer",
      "minimum": 1
    },
    "reward_off_frames": {
      "type": "integer",
      "minimum": 1
    },
    "threshold_hysteresis": {
      "type": "number",
      "minimum": 0
    },
    "dwell_seconds": {
      "type": "number",
      "minimum": 0
    },
    "refractory_seconds": {
      "type": "number",
      "minimum": 0
    },
    "feedback_mode": {
      "type": "string"
    },
    "feedback_span": {
      "type": [
        "number",
        "null"
      ]
    },
    "train_block_seconds": {
      "type": "number",
      "minimum": 0
    },
    "rest_block_seconds": {
      "type": "number",
      "minimum": 0
    },
    "start_with_rest": {
      "type": "boolean"
    },
    "window_seconds": {
      "type": "number",
      "minimum": 0
    },
    "update_seconds": {
      "type": "number",
      "minimum": 0
    },
    "metric_smooth_seconds": {
      "type": "number",
      "minimum": 0
    },
    "playback_speed": {
      "type": "number",
      "minimum": 0
    },
    "nperseg": {
      "type": "integer",
      "minimum": 1
    },
    "log10_power": {
      "type": "boolean"
    },
    "relative_power": {
      "type": "boolean"
    },
    "relative_fmin_hz": {
      "type": "number",
      "minimum": 0
    },
    "relative_fmax_hz": {
      "type": "number",
      "minimum": 0
    },
    "overlap": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    },
    "artifact_gate": {
      "type": "boolean"
    },
    "artifact_ptp_z": {
      "type": "number"
    },
    "artifact_rms_z": {
      "type": "number"
    },
    "artifact_kurtosis_z": {
      "type": "number"
    },
    "artifact_min_bad_channels": {
      "type": "integer",
      "minimum": 0
    },
    "artifact_ignore_channels": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "biotrace_ui": {
      "type": "boolean"
    },
    "export_derived_events": {
      "type": "boolean"
    },
    "derived_events_written": {
      "type": "boolean"
    },
    "derived_events_csv": {
      "type": [
        "string",
        "null"
      ]
    },
    "derived_events_tsv": {
      "type": [
        "string",
        "null"
      ]
    },
    "derived_events_json": {
      "type": [
        "string",
        "null"
      ]
    }
